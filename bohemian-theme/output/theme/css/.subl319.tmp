/***** Global *****/
*, *:before, *:after {
  -webkit-box-sizing: border-box; 
  -moz-box-sizing: border-box; 
  box-sizing: border-box;
}


/* Body */
body {
  width: 100%;
  max-width: 960px;
  margin: 0 auto;
  padding-left: 20px;
  padding-right: 20px;

  font-family: Alegreya, Calendas Plus, sans-serif;
  font-size: 18px;
  line-height: 1.4;

  -moz-font-feature-settings: "liga=1, dlig=1";
  -ms-font-feature-settings: "liga", "dlig";
  -webkit-font-feature-settings: "liga", "dlig";
  -o-font-feature-settings: "liga", "dlig";
  font-feature-settings: "liga", "dlig";
}

h1, h2, h3, h4, h5, h6,
.h1,.h2,.h3,.h4,.h5,.h6{
  font-family: Bebas Neue, Geared Slab, Roboto Black, Roboto;
  font-weight: 700;
  color:indianred;
  text-shadow: 3px 3px 0 wheat;
}

a{
  color:black;
}

a:hover{
  text-decoration: overline;
}




